							TabTitle,
							TweenInfo.new(.3, Enum.EasingStyle.Quad, Enum.EasingDirection.Out),
							{TextColor3 = Color3.fromRGB(255,255,255)}
						):Play()
					end
				end
			end
		)

		if ft == false then
			ft = true
			TabTitle.TextColor3 = Color3.fromRGB(255,255,255)
			Tab.Visible = true
			CurrentTabText.Text = text
		end

		local TabContent = {}
